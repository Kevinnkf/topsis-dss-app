<% if (messages.success) { %>
<script>
	Toast.fire({
		icon: 'success',
		title: '<%= messages.success %>'
	})
</script>
<% } else if (messages.error) { %>
<script>
	Toast.fire({
		icon: 'error',
		title: '<%= messages.error %>'
	})
</script>
<% } %>
